A_Resource_Whose_Name = ps_nde_zim_oaz

psa102.v1.text = "Ibonge iNkosi, Mphefumulo wami, lakho konke okuphakathi kwami kakubonge ibizo layo elingcwele."
psa102.v2.text = "Ibonge iNkosi, Mphefumulo wami, ungakhohlwa yilokhu ekusize ngakho."

psa145.v1.text = "Dumisa iNkosi, mphefumulo wami."
psa145.v2.text = "Ngizakuyidumisa iNkosi ngisaphila, ngizakuhlabelela uNkulunkulu wami ngisesekhona."
psa145.v5.text = "UBusisiwe yena omsizi wakhe unguNkulunkulu kaJakobe, othemba lakhe lise Nkosini uNkulunkulu wakhe."
psa145.v10.text = "INkosi izakubusa kuze kube nininini, uNkulunkulu wakho, Ziyone, kuze kube sezizukulwaneni zonke."