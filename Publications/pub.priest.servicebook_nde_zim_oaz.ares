li.rubric.showcoversstar = "Abe sesipha ipaten ilezidikiselo lenkanyezi kumdikhoni, yena okutshengisa abantu abesefaka kutafula lomhlatshelo ahambe ngemva kwetafula engcwele. Abesethukulula i-orarion."
li.rubric.blessed = "Umfundisi uyakhothama, athathe inkezo athi ngezwi eliphansi:"
