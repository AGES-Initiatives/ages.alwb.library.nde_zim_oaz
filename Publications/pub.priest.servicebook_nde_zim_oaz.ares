li.rubric.readgospel = "Umdikhoni sebala isifundo salelolanga."
li.rubric.blesswithgospel = "Umfundisi uyathatha umbhalo kumdikhoni, awuqabule ebesebusisa abantu ngawo. Awufake etafuleni engcwele. Otshumayelayo uyaqala ukutshumayeza abantu ngezwi likaNkulunkulu."
li.rubric.catechumens = "Kungalandela imikhuleko yabalungiselela ubhabhathizo (catechumens) kusinye lesinye isicelo abantu baphendula besithi: Nkosi, sihawukele."
li.rubric.cherubic = "Abahlabelayo bahlabela ihubo leCherubic kancane kancane."
li.rubric.prayercherubicsecret = "(ngemva komkhuleko umfundisi lomdikhoni bayatsho ihubo lekherubi kathathu)"
li.rubric.wearaer = "(Umfundisi aphakamise i-Aer ayifake emahlombe kamdikhoni esithi)"
li.rubric.takegifts = "(Abesethatha ipaten ivaliwe ayiphe umdikhoni ngonanzelelo lenhlonipho, yena athathe inkezo engcwele layo ivaliwe) (Lapho bebhoda bayakhuluma bentshintshana)"
li.rubric.completehymn = "(baqedise ihubo lekherubi umfundisi lomdikhoni bangene kuOthali)."
li.rubric.deaconright = "Umdikhoni ame esandleni sokudla phambi kwetafula engcwele athi kumfundisi lapho engena:"
li.rubric.priestresponse = "(Umfundisi laye engena athi kuye)"
li.rubric.placegiftsaltar = "Umfundisi abesefaka inkezo engcwele etafuleni elingcwele, athathe ipaten kumdikhoni ayifake esandleni sokunxele eceleni kwenkezo esithi:"
li.rubric.covergiftsaer = "Abesethatha izidikiselo kupaten lakunkezo azifake kucele elilodwa etafuleni engcwele. Athathe i-Aer emahlombe kamdikhoni ayembese kumpepha abeseyifaka phezu kwenkezo lepaten. Athathe impepha aphephe izipho kathathu umdikhoni esithi:"
li.rubric.rememberpriest = "(abuyisele impepha athi kumdikhoni)"
li.rubric.waveaer = "(Lapho kukhulunywa isivumo umfundisi uyaphakamisa i-Aer ayinyikinye phezu kwenkezo lepaten.Nxa kufikwa kumazwi athi wenyuka waya ezulwini uyaqabula isiphambano phakathi kwaso asigoqe asifake eceleni lamalembu)"
li.rubric.forvigilance = "Bayaguqa bobabili umfundisi aqhubeke ngezwi eliphansi:"
li.rubric.bowcommunion = "(umfundisi lomdikhoni esendaweni yakhe bayakhothama kathathu besithi)"
li.rubric.crossorarion = "Umdikhoni uyangena ku Othali, esebophile i-orarion ngendlela yesiphambano uyama esandleni sokudla sikamfundisi esithi:"
li.rubric.breakbread = "Umfundisi uyahlephuna isinkwa kuzigaba ezine esithi:"
li.rubric.placecrosswise = "Uyazifaka kupaten ngesibonakaliso sesiphambano."
li.rubric.fillchalice = "Umdikhoni uyakhombela kunkezo ngeOrarion esithi:"
li.rubric.ic = "Umfundisi uyathatha isiqephu semvana esilo IC enze isibonakaliso sesiphambano ngaso phezu kwenkezo engcwele asifake phakathi esithi:"
li.rubric.takezeon = "(uyathatha amanzi atshisayo athi kumfundisi)"
li.rubric.blesszeon = "(umfundisi awabusise esithi)"
li.rubric.pourzeon = "Umdikhoni athele amanzi atshisayo enkezweni ngesibonakaliso sesiphambano esithi:"
li.rubric.approachbody = "Uyacela uxolo kulabo abaku Othali lakubandla lonke. Asondele kutafula elingcwele athi:"
li.rubric.xc = "Uyathatha isiqephu somzimba oligugu kakhristu kusiqephu esibhalwe amabala XC athi:"
li.rubric.wipehand = "(Abe seyemukela isinkwa esingcwele ngokwesaba langonanzelelo olukhulu, esehlikihlile isandla sakhe phezu kwepaten ngesipontshi uyathi)"
li.rubric.deaconcommune = "Umdikhoni esondela uyathi:"
li.rubric.deaconcommune2 = "Umfundisi uyapha umdikhoni isiqephu sesinkwa  esingcwele kusiqephu esidindwe XC athi:"
