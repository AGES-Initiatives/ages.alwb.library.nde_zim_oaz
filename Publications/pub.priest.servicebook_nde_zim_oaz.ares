li.rubric.deaconcommune5 = "Umdikhoni esehlikihlile ngonanzelelo izandla zakhe ngesipontshi phezu kwepaten uyasondela esithi:"
li.rubric.nika = "(Umdikhoni uyahlephuna iziqephu ezimbili eziseleyo zemvana NI lo KA zibe yiziqephu ezincinyane azifake enkezweni abesevala ngelembu lesidlo. Abesefaka ukhezo phezu kwenkezo evaliweyo. Ngokuhlonipha athi)"
li.rubric.approach = "Iminyango engcwele iyavulwa, umfundisi aphe umdikhoni inkezo, aphume ngomnyango ongcwele ethwele inkezo esithi:"
li.rubric.distributionprayer = "(Umdikhoni uyaqhubela umfundisi inkezo yena opha abantu isidlo esithi)"
li.rubric.replacechalice = "(Lapho abantu sebeyemukele, umfundisi uyapha inkezo kumdikhoni, yena oyifaka etafuleni elingcwele futhi)"
li.rubric.blesshand = "Umfundisi uyabusisa abantu ngesandla sakhe esithi:"
li.rubric.wipeparticles = "Ngamadili eNkosi kuhlatshelwa i-apolitika yedili, kuthi ngephasika kuhlatshelwe eyephasika. Ngamadili eNkosi kuhlatshelwa i-apolitika yedili, kuthi ngephasika kuhlatshelwe eyephasika."
