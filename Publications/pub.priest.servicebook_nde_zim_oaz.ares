li.rubric.havingpartaken = "Umdikhoni uyaphuma phandle athi:"
li.rubric.crossantimensiongospel = "Ekhulumela phezulu esenza isibonakaliso sesiphambano ngombhalo phezu kwe-antimension egoqiweyo."
li.rubric.ambonprayer = "Umfundisi uyaphuma ngomnyango Ongcwele, phambili komfanekiso kaKristu kumbe ngaphansi kwe Solea phakathi kwebandla ebala esithiâ€¦)"
li.rubric.pliromanomou = "Umfundisi uyabuyela kuOthali ngomnyango ongcwele, ayekutafula lomhlatshelo athi ngezwi eliphansi:"
li.rubric.evlogiakyriou = "Umfundisi uyaphuma ngomnyango ongcwele abusise abantu asithi:"

li.blessreader = "Baba, busisa lo obala amazwi alungileyo kampostoli ongcwele olodumo umvangeli (ibizo) Matewu, Lukha, Johane, Makho."
li.blessreader2 = "Kwangathi uNkulunkulu ngemithandazo kampostoli ongcwele olodumo umvangeli (ibizo) angakuvumela ukubala amazwi angcwele ngamandla amakhulu, ekugcwalisekeni kombhalo weNdodana yakhe ethandekayo, iNkosi yethu uJesu Khristu."
li.cherubic2 = "Ukuze semukele iNkosi yakho konke. Ephelekezelwa okungabonakaliyo libutho lezingilosi. Aleluya x3"
li.eparon = "Baba, phakamisa."
li.holydiaconate = "Kwangathi iNkosi uNkulunkulu ingakhumbula ubudikhoni bakho embusweni wakhe, manje kuze kube nini lanini."
li.noblejoseph = "UJosefa olungileyo, esethule umzimba wakho omsulwa esihlahleni wawugoqela ngengubo ehlanzekileyo elamakha anukelelayo wasewulalisa ethuneni elitsha."
li.praypriest = "Ngithandazele, mfowethu wena osebenza lami."
li.tasa = "Silinikeza okwenu kungokwenu, kuzinto zonke zezinto zonke."
li.seimnumen = "Siyakudumisa, siyakubusisa, siyakubonga, O Nkosi, njalo siyathandaza kuwe, Nkulunkulu wethu."
li.pliroma = "Ukugcwaliseka koMoya Ongcwele."
li.zesis = "Ukufudumala koMoya Ongcwele. Amen."
li.deaconcommune = "Mdikhoni, sondela eduze."
li.deaconcommune2 = "Mdikhoni, sondela eduze futhi."
li.approachagain = "Khangela njalo ngiyasondela kuKhristu iNkosi yethu engafiyo uNkulunkulu njalo enguMsindisi, uJesu Kristu, ukuze ngithethelelwe izono ngibe lokuphila okungapheliyo."
li.etimetadidotai = "Igazi eliligugu njalo elingcwele leNkosi yethu, uNkulunkulu njalo enguMsindisi, uJesu Kristu, liphiwa wena mdikhoni ohloniphekayo (ibizo), ukuze uthethelelwe izono ube lokuphila okungapheliyo."
li.godisthelord = "Ubusisiwe lo oza egameni leNkosi. INkosi inguNkulunkulu njalo isivelile kithi."
li.distributionprayershort = "Inceku kaNkulunkulu (ibizo) iyamukela umziba legazi okuligugu njalo okungcwele kweNkosi yethu, uNkulunkulu njalo enguMsindisi, uJesu Khristu, ukuze ithethelelwe izono ibe lokuphila okungapheliyo."
li.lordsname = "Egameni leNkosi."
