li.rubric.resurrection = "(Sihlabela ingoma yosuku [apolytikion])"
li.rubric.deaconquietly2 = "Nxa umkhuleko usuphelile umdikhoni ngezwi eliphansi."
li.rubric.blessentrance = "Umfundisi ngezwi eliphansi uyabusisa ukungena."
